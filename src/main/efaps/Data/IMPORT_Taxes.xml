<import xmlns="http://www.efaps.org/xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
        
  <!-- connect the Products_MyDesk Menu to the main menu from efaps -->
  <object type="Sales_TaxCategory">
    <attribute name="Name">VAT</attribute>
    <object type="Sales_Tax">
      <parentattribute name="TaxCategory"/>
      <attribute name="Name">VAT</attribute>
      <attribute name="Numerator">19</attribute>
      <attribute name="Denominator">100</attribute>
      <attribute name="ValidFrom">2009-01-01T00:00:00.000-0500</attribute>
    </object>
  </object>
  <object type="Sales_TaxCategory">
    <attribute name="Name">TaxFree</attribute>
    <object type="Sales_Tax">
      <parentattribute name="TaxCategory"/>
      <attribute name="Name">Free</attribute>
      <attribute name="Numerator">1</attribute>
      <attribute name="Denominator">1</attribute>
      <attribute name="ValidFrom">2009-01-01T00:00:00.000-0500</attribute>
    </object>
  </object>
</import>