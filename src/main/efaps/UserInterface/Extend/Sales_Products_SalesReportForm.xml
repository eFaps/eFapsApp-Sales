    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Sales_Products_SalesReportForm</name>
        <!-- fields -->
        <field name="salesReport">
            <property name="HideLabel">true</property>
            <trigger program="org.efaps.esjp.sales.SalesProductReport"
                method="generateReport"
                name="Sales_Products_SalesReportForm.salesReport.UI_FIELD_VALUE"
                event="UI_FIELD_VALUE">
                <property name="ShowDetails">true</property>
                <property name="ContactFilter">true</property>
                <property name="DateFilter">month</property>
                <property name="Types">Sales_Invoice;Sales_Receipt</property>
                <property name="YearsAgo">1</property>
            </trigger>
        </field>
    </definition>
